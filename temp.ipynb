   "source": [
    "BATCH_SIZE = 64\n",
    "train_loader = DataLoader(train_data, batch_size=BATCH_SIZE, shuffle=True)\n",
    "test_loader  = DataLoader(test_data,  batch_size=BATCH_SIZE)\n"
   ]
  },
